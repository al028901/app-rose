{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "amd": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/no-unknown-property": [
            2,
            {
              "ignore": [
                "jsx"
              ]
            }
          ]
    }
}
